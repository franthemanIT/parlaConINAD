<xs:schema xmlns="http://inps.it/ISEERiforma" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://inps.it/ISEERiforma" elementFormDefault="qualified">
	<xs:simpleType name="TypeDataSQL">
		<xs:restriction base="xs:date">
			<xs:minInclusive value="1753-01-01"/>
			<xs:maxInclusive value="9999-12-31"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TypeCodiceFiscale">
		<xs:restriction base="xs:string">
			<xs:pattern value="([A-Za-z]{6}\d{2}[A-Za-z]\d{2}[A-Za-z][A-Za-z0-9]{3}[A-Za-z])|(\d{11} {0,5})"/>
			<!--xs:length value="16"/ -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TypeCodiceComune">
		<xs:restriction base="xs:string">
			<xs:length value="4"/>
			<xs:pattern value="[A-Z][0-9][0-9][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TypeSiglaProvincia">
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
			<xs:pattern value="[A-Z]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TypeDecimal">
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TypePercentuale">
		<xs:restriction base="xs:decimal">
			<xs:maxInclusive value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TypeProtocollo">
		<xs:restriction base="xs:string">
			<xs:pattern value="INPS-ISEE-[0-9]{4}-[0-9]{8}[A-Z]-[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--
&#x00C0; lettera A maiuscola con accento grave U+00C0
&#x00C8; lettera E maiuscola con accento grave U+00C8 
&#x00C9; lettera E maiuscola con accento acute U+00C9  
&#x00CC; lettera I maiuscola con accento grave U+00CC  
&#x00D2; lettera O maiuscola con accento grave U+00D2  
&#x00D9; lettera U maiuscola con accento grave U+00D9  
-->
	<xs:simpleType name="TypeNome">
		<xs:restriction base="xs:string">
			<xs:maxLength value="64"/>
			<xs:pattern value="[A-Z&#x00C0;&#x00C8;&#x00C9;&#x00CC;&#x00D2;&#x00D9;' -]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TypeProtocolloFlussoXML">
		<xs:restriction base="xs:string">
			<xs:pattern value="INPS-ISE[DRT]{1}-[0-9]{4}-[0-9]{8}[A-Z]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TypeStatoDomandaPrestazione">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Da Erogare"/>
			<xs:enumeration value="Erogata"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TypeTipoIndicatore">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Ordinario"/>
			<xs:enumeration value="Minorenne"/>
			<xs:enumeration value="Universitario"/>
			<xs:enumeration value="SocioSanitario"/>
			<xs:enumeration value="Dottorato"/>
			<xs:enumeration value="Residenziale"/>
